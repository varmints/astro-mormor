{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "importsNotUsedAsValues": "remove"
  },
  "include": ["**/*", ".eslintrc.cjs"],
  "exclude": ["node_modules", "dist", ".jampack", ".astro"]
}
